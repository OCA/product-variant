<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets_backend_inherit_product_matrix_sticky_header"
        inherit_id="web.assets_backend"
        name="Assets Product Matrix Sticky Header"
    >
        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/product_matrix_sticky_header/static/src/scss/product_matrix.scss"
            />
        </xpath>
        <xpath expr="script[last()]" position="after">
            <script
                type="text/javascript"
                src="/product_matrix_sticky_header/static/src/js/section_and_note_widget.js"
            />
        </xpath>
    </template>
</odoo>
